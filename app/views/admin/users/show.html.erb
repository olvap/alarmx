<div class="row">
  <div class="col s12">
    <h3><%= @user.email %></h3>
    <%= link_to 'Editar', edit_admin_user_path(@user), class: 'btn btn-small' %>
    <%= link_to 'Eliminar', admin_user_path(@user), method: :delete, data: { confirm: '¿Estás seguro de que quieres eliminar este usuario?' }, class: 'btn btn-small red darken-2' %>
  </div>
</div>

