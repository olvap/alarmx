<%= form_with(model: [:admin, @user], html: {class: "form"}) do |f| %>
  <div class="row">
    <div class="col s12 input-field">
      <%= f.label :email %>
      <%= f.text_field :email, id: :email %>
    </div>
  </div>
  <div class="row">
    <div class="col s12">
      <%= f.submit class: "btn" %>
    </div>
  </div>
<% end %>

