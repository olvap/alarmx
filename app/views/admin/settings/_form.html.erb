<%= form_with(model: [:admin, @setting], html: {class: "form"}) do |f| %>
  <div class="row">
    <div class="col s12 input-field">
      <%= f.label :chat_id %>
      <%= f.text_field :chat_id, id: :chat_id %>
    </div>
  </div>
  <div class="row">
    <div class="col s12 input-field">
      <%= f.label :user_id %>
      <%= f.text_field :user_id, id: :user_id %>
    </div>
  </div>
  <div class="row">
    <div class="col s12">
      <%= f.submit class: "btn" %>
    </div>
  </div>
<% end %>

