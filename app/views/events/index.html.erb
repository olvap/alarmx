<h2>Events</h2>

<ul class="collection">
  <% @events.each do |event| %>
    <li class="collection-item">
      <%= link_to event.name, event_path(event) %>
    </li>
  <% end %>
</ul>
<div class="right-align">
  <%= link_to new_event_path, class: "btn-floating waves-effect waves-light" do %>
  <i class="material-icons">add</i>
<% end %>
</div>
